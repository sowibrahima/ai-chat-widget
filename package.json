{
  "name": "@wutiskill/ai-chat-widget",
  "version": "1.0.0-semantically-released",
  "description": "Floating AI chat widget for Open edX MFEs (plugin-ready)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sowibrahima/ai-chat-widget.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "make build",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "wutiskill",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/sowibrahima/ai-chat-widget#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sowibrahima/ai-chat-widget/issues"
  },
  "dependencies": {
    "classnames": "2.5.1",
    "react-markdown": "^9.0.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@openedx/frontend-build": "14.4.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "husky": "7.0.4",
    "jest": "29.7.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^8.0.0",
    "@openedx/paragon": "^23.0.0",
    "prop-types": "^15.5.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  }
}
